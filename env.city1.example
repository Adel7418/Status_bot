# Пример .env для первого бота (city1)

# Обязательно заменить на реальный токен
BOT_TOKEN=

# Список ID админов через запятую
ADMIN_IDS=123456789

# (опционально) список диспетчеров через запятую
DISPATCHER_IDS=

# Режим продакшена
DEV_MODE=false

# Путь к БД внутри контейнера (не менять)
DATABASE_PATH=/app/data/bot_database.db

# Логирование
LOG_LEVEL=INFO

# Использование ORM (при необходимости)
USE_ORM=false

# Интервалы напоминаний/SLA
SLA_CHECK_INTERVAL=30
REMINDER_INTERVAL=5
